{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MaxLoginAttempts": 4,
  "LockoutAutoResetMinutes": 15,
  "JwtToken": {
    "accessTokenExpiration": 20,
    "refreshTokenExpiration": 360
  },
  "KeyVaultName": "shoporiumwebvault",
  "AllowCorsFromOrigin": [ "http://localhost:4200", "https://shoporiumui.azurewebsites.net" ]
}
